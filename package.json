{
  "name": "badge-generator",
  "version": "1.8.0",
  "description": "Magically generate custom badges that you can paste in your docs",
  "author": "MichaelCurrin",
  "scripts": {
    "lint:fix": "vue-cli-service lint",
    "lint:check": "vue-cli-service lint --no-fix",
    "serve": "vue-cli-service serve",
    "start": "yarn lint:fix && yarn run serve",
    "compile": "rm -rf dist && tsc -p . --outDir dist --sourceMap false",
    "build": "vue-cli-service build",
    "preversion": "yarn lint:check",
    "version": "yarn build",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.0.3",
    "core-js": "^3.8.0",
    "vue": "^3.0.3",
    "vue-markdown": "^2.2.4",
    "vue-router": "^4.0.0-rc.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-plugin-router": "^4.5.6",
    "@vue/cli-plugin-typescript": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "babel-runtime": "^6.26.0",
    "eslint": "6.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.1.1",
    "typescript": "^4.1.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "homepage": "https://github.com/MichaelCurrin/badge-generator#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MichaelCurrin/badge-generator"
  }
}
